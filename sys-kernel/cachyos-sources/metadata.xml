  <use>
    <flag name="bore-eevdf">
      Use EEVDF combined with BORE scheduler. The default set of CachyOS
    </flag>
    <flag name="eevdf">
      Use EEVDF(Earliest Eligible Virtual Deadline)
    </flag>
    <flag name="pds">
      Use Priority and Deadline based Skiplist multiple queue by Alfred Chen
    </flag>
    <flag name="bmq">
      Use BitMap Queue by Alfred Chen
    </flag>
    <flag name="tt">
      Use Task Type Scheduler by Hamad Marri
    </flag>
    <flag name="bore">
      Use BORE(Burst-Oriented Response Enhancer) by firelzrd
    </flag>
    <flag name="cfs">
      Use Standard Scheduler Completely Fair Scheduler
    </flag>
    <flag name="cfs-rt">
      Use CFS with realtime patches
    </flag>
    <flag name="bore-hardened">
      Use BORE with hardened patches
    </flag>
    <flag name="bcachefs">
      Enable bcachefs
    </flag>
    <flag name="lrng">
      Enable Linux Random Number Generator
    </flag>
  </use>
</pkgmetadata>
